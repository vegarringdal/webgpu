{
    "name": "webgpu",
    "version": "1.0.0",
    "description": "Experiment with webgpu",
    "scripts": {
        "start": "run-p -s -l start:*",
        "start:frontend": "vite",
        "start:typechecker": "npm run typecheck:watch",
        "build": "run-s -s -n build:*",
        "build:frontend": "vite build",
        "eslint": "run-s -s -n eslint:nofix:*",
        "eslint:nofix:frontend": "eslint ./src/**/*.{js,ts,tsx}",
        "eslint:fix": "run-s -s -n eslint:fix:*",
        "eslint:fix:frontend": "eslint --fix ./src/**/*.{js,ts,tsx}",
        "prettier": "run-s -s -n prettier:*",
        "prettier:json": "prettier --write  \"./**/*.json\"",
        "prettier:ts": "prettier --write  \"./**/*.ts\"",
        "prettier:js": "prettier --write  \"./**/*.js\"",
        "prettier:tsx": "prettier --write  \"./**/*.tsx\"",
        "typecheck": "node ./typecheck.js",
        "typecheck:watch": "node ./typecheck_watch.js"
    },
    "keywords": [],
    "author": "vegar.ringdal@gmail.com",
    "license": "todo",
    "devDependencies": {
        "@types/react": "^18.0.18",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "@vitejs/plugin-react": "^2.0.1",
        "@webgpu/types": "^0.1.21",
        "autoprefixer": "^10.4.8",
        "camera-controls": "^1.36.2",
        "comlink": "^4.3.1",
        "esbuild": "^0.15.6",
        "esbuild-helpers": "^0.0.9",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "fflate": "^0.7.3",
        "gl-matrix": "^3.4.3",
        "husky": "^8.0.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.16",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.8.2",
        "vite": "^3.0.9",
        "zustand": "^4.1.1"
    }
}
